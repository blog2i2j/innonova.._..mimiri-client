{
	"name": "webclient-vue",
	"version": "2.5.47",
	"minElectronVersionWin32": "2.5.46",
	"minElectronVersionDarwin": "2.5.46",
	"minElectronVersionLinux": "2.5.46",
	"minIosVersion": "2.3.1",
	"minAndroidVersion": "2.3.1",
	"iosVersion": "2.5.12",
	"androidVersion": "2.5.12",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite --host",
		"prettier": "prettier --end-of-line auto --check \"**/*.(ts|js|cjs|mjs|vue)\"",
		"build": "run-p type-check \"build-only {@}\" --",
		"preview": "vite preview",
		"build-only": "vite build",
		"build-dev": "NODE_ENV=development vite build",
		"type-check": "vue-tsc --build --force",
		"test": "jest",
		"e2e": "playwright test --retries=0",
		"e2e-ss-update": "playwright test --update-snapshots",
		"icons-import": "node ./scripts/icons",
		"icons-list": "node ./scripts/create-icon-list.js",
		"cap-sync": "npx cap sync",
		"cap-sync-dev": "NODE_ENV=development npx cap sync",
		"ios": "npx cap open ios",
		"android": "npx cap open android",
		"sync": "run-s build cap-sync",
		"sync-dev": "run-s build-dev cap-sync-dev",
		"sync-prod": "run-s download-bundle unpack-bundle update-version-info cap-sync",
		"update-bundle": "node scripts/update-bundle.js",
		"download-bundle": "node scripts/download-bundle.js",
		"unpack-bundle": "node scripts/unpack-bundle.js",
		"update-version-info": "node scripts/update-version-info.js",
		"set-version": "node ./scripts/set-version",
		"make-bundle": "node ./scripts/make-bundle",
		"create-key": "node ./scripts/create-key",
		"deploy-web": "node ./scripts/deploy-web",
		"demo": "npx playwright test -c playwright.demo.config.ts",
		"process-screenshots": "node ./scripts/process-screenshots.js",
		"create-screenshots": "npm run demo && npm run process-screenshots"
	},
	"dependencies": {
		"@capacitor/android": "^7.4.3",
		"@capacitor/app": "^7.1.0",
		"@capacitor/core": "^7.4.3",
		"@capacitor/ios": "^7.4.3",
		"@capacitor/keyboard": "^7.0.3",
		"@microsoft/signalr": "^9.0.6",
		"@vueuse/core": "13.9.0",
		"assert": "2.1.0",
		"date-fns": "^4.1.0",
		"idb": "^8.0.3",
		"js-base64": "^3.7.8",
		"monaco-editor": "^0.53.0",
		"node-diff3": "^3.1.2",
		"uuid": "^13.0.0",
		"vue": "^3.5.22",
		"vue-router": "^4.5.1",
		"zxcvbn": "^4.4.2"
	},
	"devDependencies": {
		"@capacitor/assets": "^3.0.5",
		"@capacitor/cli": "^7.4.3",
		"@jest/globals": "^30.2.0",
		"@playwright/test": "1.55.1",
		"@tailwindcss/postcss": "^4.1.13",
		"@tsconfig/node18": "18.2.4",
		"@types/crypto-js": "4.2.2",
		"@types/node": "24.6.1",
		"@types/uuid": "^11.0.0",
		"@typescript-eslint/eslint-plugin": "^8.45.0",
		"@typescript-eslint/parser": "^8.45.0",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vitejs/plugin-vue-jsx": "^5.1.1",
		"@vue/eslint-config-typescript": "^14.6.0",
		"@vue/tsconfig": "0.8.1",
		"dotenv": "^17.2.3",
		"eslint": "9.36.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-prettier-vue": "5.0.0",
		"eslint-plugin-vue": "10.5.0",
		"html-entities": "^2.6.0",
		"jest": "^30.2.0",
		"jest-environment-jsdom": "^30.2.0",
		"jimp": "^1.6.0",
		"sharp": "^0.34.4",
		"moment": "2.30.1",
		"npm-run-all2": "8.0.4",
		"postcss": "^8.5.6",
		"prettier": "3.6.2",
		"shelljs": "^0.10.0",
		"tailwindcss": "^4.1.13",
		"ts-jest": "^29.4.4",
		"typescript": "5.9.3",
		"vite": "^7.1.7",
		"vite-plugin-vue-devtools": "^8.0.2",
		"vue-tsc": "^3.1.0"
	},
	"overrides": {
		"sharp": {
			"tar-fs": "^3.0.9"
		},
		"prebuild-install": {
			"tar-fs": "^2.1.3"
		}
	}
}
